<p><s id="s1"><w id="w32769">We</w> <w id="w32770">found</w> <w id="w32771">that</w> <w id="w32772">the</w> <w id="w32773">expression</w> <w id="w32774">of</w> <w id="w32775">all</w> <w id="w32776">these</w> <w id="w32777">genes</w> <w id="w32778">,</w> <w id="w32779">including</w> <w id="w32780">thymidylate</w> <w id="w32781">synthase</w> <w id="w32782">(</w> <w id="w32783">TS</w> <w id="w32784">)</w> <w id="w32785">,</w> <w id="w32786">dihydrofolate</w> <w id="w32787">reductase</w> <w id="w32788">(</w> <w id="w32789">DHFR</w> <w id="w32790">)</w> <w id="w32791">,</w> <w id="w32792">ribonucleotide</w> <w id="w32793">reductase</w> <w id="w32794">(</w> <w id="w32795">PNR</w> <w id="w32796">)</w> <w id="w32797">,</w> <w id="w32798">proliferating</w> <w id="w32799">cell</w> <w id="w32800">nuclear</w> <w id="w32801">antigen</w> <w id="w32802">(</w> <w id="w32803">PCNA</w> <w id="w32804">)</w> <w id="w32805">,</w> <w id="w32806">histone</w> <w id="w32807">H1</w> <w id="w32808">,</w> <w id="w32809">histone</w> <w id="w32810">H</w><w id="w32811">2A</w> <w id="w32812">+</w> <w id="w32813">2B</w> <w id="w32814">,</w> <w id="w32815">histone</w> <w id="w32816">H3</w> <w id="w32817">,</w> <w id="w32818">and</w> <w id="w32819">histone</w> <w id="w32820">H4</w> <w id="w32821">,</w> <w id="w32822">was</w> <w id="w32823">induced</w> <w id="w32824">to</w> <w id="w32825">high</w> <w id="w32826">levels</w> <w id="w32827">in</w> <w id="w32828">young</w> <w id="w32829">IMR-90</w> <w id="w32830">cells</w> <w id="w32831">but</w> <w id="w32832">not</w> <w id="w32833">in</w> <w id="w32834">old</w> <w id="w32835">IMR-90</w> <w id="w32836">cells</w> <w id="w32837">.</w></s></p>
