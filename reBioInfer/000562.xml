<p><s id="s1"><w id="w7912">Here</w> <w id="w7913">,</w> <w id="w7914">specific</w> <w id="w7915">interactions</w> <w id="w7916">between</w> <w id="w7917">human</w> <w id="w7918">RAD51</w> <w id="w7919">and</w> <w id="w7920">RAD52</w> <w id="w7921">proteins</w> <w id="w7922">are</w> <w id="w7923">demonstrated</w> <w id="w7924">both</w> <w id="w7925">in</w> <w id="w7926">vivo</w> <w id="w7927">,</w> <w id="w7928">using</w> <w id="w7929">the</w> <w id="w7930">yeast</w> <w id="w7931">two-hybrid</w> <w id="w7932">system</w> <w id="w7933">and</w> <w id="w7934">immunoprecipitation</w> <w id="w7935">of</w> <w id="w7936">insect</w> <w id="w7937">cells</w> <w id="w7938">co-infected</w> <w id="w7939">with</w> <w id="w7940">RAD51</w> <w id="w7941">and</w> <w id="w7942">RAD52</w> <w id="w7943">recombinant</w> <w id="w7944">viruses</w> <w id="w7945">,</w> <w id="w7946">and</w> <w id="w7947">in</w> <w id="w7948">vitro</w> <w id="w7949">,</w> <w id="w7950">using</w> <w id="w7951">affinity</w> <w id="w7952">chromatography</w> <w id="w7953">with</w> <w id="w7954">purified</w> <w id="w7955">recombinant</w> <w id="w7956">proteins</w> <w id="w7957">.</w></s></p>
