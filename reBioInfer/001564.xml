<p><s id="s1"><w id="w24765">The</w> <w id="w24766">muscle-related</w> <w id="w24767">genes</w> <w id="w24768">included</w> <w id="w24769">actin</w> <w id="w24770">,</w> <w id="w24771">tropomyosin</w> <w id="w24772">,</w> <w id="w24773">troponin</w> <w id="w24774">I</w> <w id="w24775">,</w> <w id="w24776">myosin</w> <w id="w24777">regulatory</w> <w id="w24778">light</w> <w id="w24779">chain</w> <w id="w24780">,</w> <w id="w24781">myosin</w> <w id="w24782">light</w> <w id="w24783">chain</w> <w id="w24784">kinase</w> <w id="w24785">,</w> <w id="w24786">myosin</w> <w id="w24787">heavy</w> <w id="w24788">chain</w> <w id="w24789">,</w> <w id="w24790">calmodulin</w> <w id="w24791">,</w> <w id="w24792">calponin</w> <w id="w24793">,</w> <w id="w24794">calcium</w> <w id="w24795">vector</w> <w id="w24796">protein</w> <w id="w24797">,</w> <w id="w24798">creatine</w> <w id="w24799">kinase</w> <w id="w24800">,</w> <w id="w24801">muscle</w> <w id="w24802">LIM</w> <w id="w24803">protein</w> <w id="w24804">,</w> <w id="w24805">and</w> <w id="w24806">SH3-binding</w> <w id="w24807">glutamate-rich</w> <w id="w24808">protein</w> <w id="w24809">,</w> <w id="w24810">suggesting</w> <w id="w24811">that</w> <w id="w24812">vertebrate</w> <w id="w24813">skeletal</w> <w id="w24814">and</w> <w id="w24815">smooth</w> <w id="w24816">muscle-type</w> <w id="w24817">genes</w> <w id="w24818">are</w> <w id="w24819">simultaneously</w> <w id="w24820">expressed</w> <w id="w24821">in</w> <w id="w24822">the</w> <w id="w24823">amphioxus</w> <w id="w24824">notochord</w> <w id="w24825">.</w></s></p>
