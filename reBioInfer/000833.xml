<p><s id="s1"><w id="w12874">Interestingly</w> <w id="w12875">,</w> <w id="w12876">two</w> <w id="w12877">major</w> <w id="w12878">actin-binding</w> <w id="w12879">proteins</w> <w id="w12880">found</w> <w id="w12881">in</w> <w id="w12882">brain</w> <w id="w12883">,</w> <w id="w12884">gelsolin</w> <w id="w12885">and</w> <w id="w12886">profilin</w> <w id="w12887">,</w> <w id="w12888">specifically</w> <w id="w12889">interact</w> <w id="w12890">with</w> <w id="w12891">phosphatidylinositol</w> <w id="w12892">4,5-bisphosphate</w> <w id="w12893">,</w> <w id="w12894">and</w> <w id="w12895">may</w> <w id="w12896">themselves</w> <w id="w12897">play</w> <w id="w12898">a</w> <w id="w12899">role</w> <w id="w12900">in</w> <w id="w12901">regulating</w> <w id="w12902">phosphoinositide</w> <w id="w12903">turnover</w> <w id="w12904">.</w></s></p>
